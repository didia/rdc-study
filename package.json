{
  "name": "rdc-study",
  "description": "rdc-study project",
  "version": "1.8.7",
  "author": "Aristote Diasonama <thefuture2092@gmail.com>",
  "keywords": [
    "rdc-study"
  ],
  "main": "gatsby-browser.js",
  "license": "rdc-study",
  "engines": {
    "node": "^10.0.0",
    "npm": "^5.6"
  },
  "scripts": {
    "build": "gatsby build",
    "deploy-qa": "gatsby build && surge public/ $SURGE_DOMAIN",
    "start": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "./node_modules/.bin/stylelint './src/**/*.scss' && ./node_modules/.bin/eslint './src/**/*.js'",
    "prettier": "prettier --write 'src/**/*.{js,json,scss,md}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "newsletter-subscriber-count": "aws dynamodb scan --table-name newsletter-subscriptions --select \"COUNT\"",
    "update-contact-email-template": "aws ses update-template --cli-input-json file://aws/contact-form-email-template.json --region=us-east-1",
    "update-newsletter-email-template": "aws ses update-template --cli-input-json file://aws/newsletter-operation-failed-email-template.json --region=us-east-1"
  },
  "lint-staged": {
    "*.{js,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@material/button": "^3.2.0",
    "@material/elevation": "^3.1.0",
    "@material/react-button": "^0.15.0",
    "@material/react-card": "^0.15.0",
    "@material/react-text-field": "^0.15.0",
    "@material/react-typography": "^0.15.0",
    "@material/ripple": "^3.2.0",
    "@material/textfield": "^3.2.0",
    "@material/typography": "^3.1.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.5",
    "date-fns": "^2.8.1",
    "gatsby": "^2.18.3",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-catch-links": "^2.1.17",
    "gatsby-plugin-feed": "^2.3.21",
    "gatsby-plugin-google-tagmanager": "^2.1.17",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-netlify": "^2.1.26",
    "gatsby-plugin-netlify-cms": "^4.1.28",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "gatsby-plugin-postcss": "^2.1.15",
    "gatsby-plugin-react-helmet": "^3.1.15",
    "gatsby-plugin-remove-trailing-slashes": "^2.1.14",
    "gatsby-plugin-sass": "^2.1.23",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^2.3.3",
    "gatsby-plugin-sitemap": "^2.2.22",
    "gatsby-remark-copy-linked-files": "^2.1.30",
    "gatsby-remark-images": "^3.1.33",
    "gatsby-remark-prismjs": "^3.3.24",
    "gatsby-remark-smartypants": "^2.1.16",
    "gatsby-source-filesystem": "^2.1.38",
    "gatsby-transformer-json": "^2.2.20",
    "gatsby-transformer-remark": "^2.6.37",
    "gatsby-transformer-sharp": "^2.3.5",
    "lodash": "^4.17.15",
    "netlify-cms-app": "^2.9.7",
    "node-sass": "^4.13.0",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-intl": "^3.7.0",
    "s3-deploy": "^1.4.0",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "sharethis-reactjs": "^1.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-mirego": "0.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "stylelint": "^12.0.0",
    "stylelint-mirego": "^0.2.2",
    "stylelint-order": "^3.1.1"
  },
  "browserslist": [
    "> 1%",
    "> 1% in CD",
    "IE >= 9",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
