    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-mirego": "0.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "npm": "^6.12.0",
    "prettier": "^1.18.2",
    "stylelint": "^11.1.1",
    "stylelint-mirego": "^0.2.2",
    "stylelint-order": "^3.1.1"
