    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-json": "^2.2.6",
    "gatsby-transformer-remark": "^2.6.19",
    "gatsby-transformer-sharp": "^2.2.12",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-import": "^12.0.1",
