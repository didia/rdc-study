    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-mirego": "0.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "npm": "^6.4.1",
    "prettier": "^1.11.1",
    "stylelint": "^9.1.3",
    "stylelint-mirego": "^0.2.2",
    "stylelint-order": "^0.8.1"
  },
  "browserslist": [
    "> 1%",
    "> 1% in CD",
    "IE >= 9",
    "last 2 versions"
