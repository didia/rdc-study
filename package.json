{
  "name": "rdc-study",
  "description": "rdc-study project",
  "version": "1.10.0",
  "author": "Aristote Diasonama <thefuture2092@gmail.com>",
  "keywords": [
    "rdc-study"
  ],
  "main": "gatsby-browser.js",
  "license": "rdc-study",
  "engines": {
    "node": "^10.0.0",
    "npm": "^5.6"
  },
  "scripts": {
    "build": "gatsby build",
    "build-with-bundle-analyzer": "WITH_BUNDLE_ANALYZER=true gatsby build",
    "deploy-qa": "gatsby build && surge public/ $SURGE_DOMAIN",
    "start": "SHOW_DRAFT=true gatsby develop",
    "develop": "SHOW_DRAF=true gatsby clean && gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "./node_modules/.bin/stylelint './src/**/*.scss' && ./node_modules/.bin/eslint './src/**/*.js'",
    "prettier": "prettier --write 'src/**/*.{js,json,scss,md}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "newsletter-subscriber-count": "aws dynamodb scan --table-name newsletter-subscriptions --select \"COUNT\"",
    "update-contact-email-template": "aws ses update-template --cli-input-json file://aws/contact-form-email-template.json --region=us-east-1",
    "update-newsletter-email-template": "aws ses update-template --cli-input-json file://aws/newsletter-operation-failed-email-template.json --region=us-east-1"
  },
  "lint-staged": {
    "*.{js,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.20.0",
    "classnames": "^2.2.5",
    "date-fns": "^2.16.1",
    "gatsby": "^2.24.53",
    "gatsby-image": "^2.4.16",
    "formik": "^2.1.6",
    "gatsby-plugin-catch-links": "^2.3.9",
    "gatsby-plugin-feed": "^2.5.9",
    "gatsby-plugin-google-tagmanager": "^2.3.11",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-netlify": "^2.3.13",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-netlify-cms": "^4.3.12",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "gatsby-plugin-postcss": "^2.3.8",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-remove-trailing-slashes": "^2.3.9",
    "gatsby-plugin-sass": "^2.3.9",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-plugin-sitemap": "^2.4.12",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-react-router-scroll": "^3.0.12",
    "gatsby-remark-copy-linked-files": "^2.3.13",
    "gatsby-remark-images": "^3.3.28",
    "gatsby-remark-prismjs": "^3.5.11",
    "gatsby-remark-smartypants": "^2.3.8",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-json": "^2.4.9",
    "gatsby-transformer-remark": "^2.8.32",
    "gatsby-transformer-sharp": "^2.5.14",
    "lodash": "^4.17.20",
    "netlify-cms-app": "^2.12.20",
    "node-sass": "^4.14.1",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.8.0",
    "react-transition-group": "^4.4.1",
    "recoil": "0.0.10",
    "s3-deploy": "^1.4.0",
    "sass-loader": "^10.0.2",
    "sass-resources-loader": "^2.1.0",
    "sharethis-reactjs": "^1.6.0",
    "yup": "^0.29.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-mirego": "0.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "stylelint": "^13.7.0",
    "stylelint-mirego": "^0.2.5",
    "stylelint-order": "^4.1.0"
  },
  "browserslist": [
    "> 1%",
    "> 1% in CD",
    "IE >= 9",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
