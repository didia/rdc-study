{
  "name": "rdc-study",
  "description": "rdc-study project",
  "version": "1.10.0",
  "author": "Aristote Diasonama <thefuture2092@gmail.com>",
  "keywords": [
    "rdc-study"
  ],
  "main": "gatsby-browser.js",
  "license": "rdc-study",
  "engines": {
    "node": "^10.0.0",
    "npm": "^5.6"
  },
  "scripts": {
    "build": "gatsby build",
    "build-with-bundle-analyzer": "WITH_BUNDLE_ANALYZER=true gatsby build",
    "deploy-qa": "gatsby build && surge public/ $SURGE_DOMAIN",
    "start": "SHOW_DRAFT=true gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "./node_modules/.bin/stylelint './src/**/*.scss' && ./node_modules/.bin/eslint './src/**/*.js'",
    "prettier": "prettier --write 'src/**/*.{js,json,scss,md}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "newsletter-subscriber-count": "aws dynamodb scan --table-name newsletter-subscriptions --select \"COUNT\"",
    "update-contact-email-template": "aws ses update-template --cli-input-json file://aws/contact-form-email-template.json --region=us-east-1",
    "update-newsletter-email-template": "aws ses update-template --cli-input-json file://aws/newsletter-operation-failed-email-template.json --region=us-east-1"
  },
  "lint-staged": {
    "*.{js,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "classnames": "^2.2.5",
    "date-fns": "^2.10.0",
    "gatsby": "^2.19.10",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-catch-links": "^2.1.21",
    "gatsby-plugin-feed": "^2.3.27",
    "gatsby-plugin-google-tagmanager": "^2.1.24",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-netlify": "^2.1.31",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-netlify-cms": "^4.1.37",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "gatsby-plugin-postcss": "^2.1.20",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-remove-trailing-slashes": "^2.1.20",
    "gatsby-plugin-sass": "^2.1.29",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^2.3.10",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-copy-linked-files": "^2.1.37",
    "gatsby-remark-images": "^3.1.43",
    "gatsby-remark-prismjs": "^3.3.30",
    "gatsby-remark-smartypants": "^2.1.19",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-json": "^2.2.25",
    "gatsby-transformer-remark": "^2.6.49",
    "gatsby-transformer-sharp": "^2.3.13",
    "lodash": "^4.17.15",
    "netlify-cms-app": "^2.11.26",
    "node-sass": "^4.13.1",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "prismjs": "^1.19.0",
    "react": "^16.13.0",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-intl": "^3.12.0",
    "s3-deploy": "^1.4.0",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.0.1",
    "sharethis-reactjs": "^1.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-mirego": "0.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "stylelint": "^12.0.1",
    "stylelint-mirego": "^0.2.2",
    "stylelint-order": "^4.0.0"
  },
  "browserslist": [
    "> 1%",
    "> 1% in CD",
    "IE >= 9",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
