    "babel-eslint": "^10.0.3",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-mirego": "0.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "npm": "^6.11.2",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-mirego": "^0.2.2",
    "stylelint-order": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "> 1% in CD",
    "IE >= 9",
    "last 2 versions"
