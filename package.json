  "scripts": {
    "build": "gatsby build",
    "deploy-qa": "gatsby build && surge public/ $SURGE_DOMAIN",
    "start": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "./node_modules/.bin/stylelint './src/**/*.scss' && ./node_modules/.bin/eslint './src/**/*.js'",
    "prettier": "prettier --write 'src/**/*.{js,json,scss,md}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-contact-email-template": "aws ses update-template --cli-input-json file://aws/email/email-template.json --region=us-east-1"
  },
  "lint-staged": {
    "*.{js,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "aws-cloudfront-invalidate": "^0.1.3",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "gatsby": "^2.0.55",
